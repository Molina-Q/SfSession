{% extends 'base.html.twig' %}


    
{% block title %}{{ user }} profile{% endblock %}

{% block body %}
<h1>{{ user }}</h1>
<ul>
    <li>{{ user.formatedBirthDate }} ({{ user.showAge }})</li>
    <li>{{ user.email }}</li>
    <li>{{ user.adress }}</li>
    <li>{{ user.postcode }}, {{ user.city }}</li>
    
</ul>
{% endblock %}


